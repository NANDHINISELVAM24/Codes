import java.util.Scanner;
class Codechef
{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int fact = 1;
        
        do{
            fact *= n;
            n--;
        } while (n > 0);
        
        System.out.println(fact);
    }
}
